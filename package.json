{
  "name": "@buildlayer/ai-cli",
  "version": "0.1.3",
  "description": "Command-line interface for AI UI SDK development and scaffolding",
  "keywords": [
    "ai",
    "cli",
    "chat",
    "openai",
    "anthropic",
    "mistral",
    "grok",
    "react",
    "typescript",
    "scaffolding",
    "generator",
    "buildlayer",
    "ai-ui",
    "chatbot",
    "llm",
    "machine-learning",
    "developer-tools",
    "boilerplate",
    "template"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/BuildLayer/ai-cli"
  },
  "homepage": "https://buildlayer.dev/ai-ui",
  "bugs": {
    "url": "https://github.com/BuildLayer/ai-cli/issues"
  },
  "bin": {
    "ai-cli": "./dist/cli.js",
    "ai": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@buildlayer/ai-core": "0.1.2",
    "@buildlayer/ai-react": "0.1.7",
    "commander": "^11.0.0",
    "chalk": "^5.0.0",
    "inquirer": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/inquirer": "^9.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.0.0"
  }
}
